      <Button fx:id="getEnergyButton" disable="true" layoutX="418.0" layoutY="60.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="129.0" text="Get Energy" />
      <Button fx:id="disconnectButton" disable="true" layoutX="418.0" layoutY="148.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="129.0" text="Disconnect" />
      <Button fx:id="connectButton" layoutX="418.0" layoutY="14.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="129.0" text="Connect" />
      <Label fx:id="statusLabel" layoutX="300.0" layoutY="358.0" text="Status: Disconnected" textFill="#e1200f" />
   </children>
</AnchorPane>
